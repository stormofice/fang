{
  "manifest_version": 2,
  "name": "Werfer",
  "version": "0.1",
  "description": "",
  "homepage_url": "https://a.b.de",
  "icons": {
    "64": "icons/ext.svg"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/save.svg",
    "default_title": "Save",
    "default_area": "navbar"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "werfer@test.temp"
    }
  }
}